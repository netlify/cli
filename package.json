    "proxyquire": "^2.1.3",
    "seedrandom": "^3.0.5",
    "serialize-javascript": "^6.0.0",
    "sinon": "^12.0.0",
    "sort-on": "^4.1.0",
    "strip-ansi": "^6.0.0",
    "supertest": "^6.1.6",
    "temp-dir": "^2.0.0",
    "tomlify-j0.4": "^3.0.0",
    "tree-kill": "^1.2.2",
    "typescript": "^4.4.4",
    "verdaccio": "^5.4.0"
  },
  "ava": {
    "files": [
      "site/**/*.test.js",
      "src/**/*.test.js",
      "tools/**/*.test.js",
      "tests/*.test.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "timeout": "5m"
  }
}
