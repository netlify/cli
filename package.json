  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/scripts/postinstall.js",
    "/src/**/*.js",
    "/src/**/*.sh",
    "/src/**/*.ps1",
    "/src/functions-templates/**",
    "/src/lib/functions/templates/**",
    "!/src/**/node_modules/**",
    "!/src/**/*.test.js"
  ],
  "homepage": "https://github.com/netlify/cli",
