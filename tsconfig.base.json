{
  "extends": ["@tsconfig/recommended", "@tsconfig/node18"],
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "typeRoots": ["node_modules/@types", "types"]
  },
  "exclude": ["**/__fixtures__/**", "dist/", "functions-templates/", "site/"]
}
