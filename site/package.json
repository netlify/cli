{
  "name": "cli-docs-site",
  "version": "1.0.0",
  "description": "Docs site for Netlify CLI",
  "private": true,
  "type": "commonjs",
  "author": "Netlify Inc.",
  "files": [
    "/src",
    "/dist"
  ],
  "scripts": {
    "start": "npm run sync && run-p start:*",
    "build": "npm run clean && run-s build:*",
    "postbuild": "cp src/_redirects dist/_redirects",
    "build:docs": "node scripts/docs.mjs",
    "build:site": "npm run sync && x0 build src",
    "start:site": "x0 src",
    "start:watch": "node ./watch.mjs",
    "watch": "node ./watch.mjs",
    "sync": "node ./sync.mjs",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@compositor/x0": "6.0.7",
    "@rebass/markdown": "1.0.0",
    "@rebass/mdx": "1.0.0",
    "algoliasearch": "4.17.1",
    "lodash.sortby": "4.7.0",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "react-instantsearch-dom": "6.39.1",
    "react-router-dom": "4.3.1",
    "react-emotion": "9.2.12",
    "rebass": "2.3.4",
    "styled-components": "3.4.10",
    "styled-system": "3.2.1"
  },
  "devDependencies": {
    "markdown-magic": "2.6.1",
    "npm-run-all": "4.1.5",
    "sane": "5.0.1",
    "strip-ansi": "7.0.1"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
