{
  "name": "cli-docs-site",
  "version": "1.0.0",
  "description": "Docs site for Netlify CLI",
  "private": true,
  "author": "Netlify Inc.",
  "contributors": [
    "David Wells <david.wells@netlify.com> (https://davidwells.io/)",
    "Bret Comnes <bcomnes@gmail.com> (https://bret.io)"
  ],
  "files": [
    "/src",
    "/dist"
  ],
  "scripts": {
    "start": "npm run sync && run-p start:*",
    "build": "npm run clean && run-s build:*",
    "postbuild": "cp src/_redirects dist/_redirects",
    "build:docs": "node scripts/docs.js",
    "build:site": "npm run sync && x0 build src",
    "start:site": "x0 src",
    "start:watch": "node ./watch.js",
    "watch": "node ./watch.js",
    "sync": "node ./sync.js",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": ">=v12.19.0"
  },
  "license": "MIT",
  "dependencies": {
    "@compositor/x0": "^6.0.7",
    "@rebass/markdown": "^1.0.0",
    "@rebass/mdx": "^1.0.0",
    "lodash.sortby": "^4.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^5.7.0",
    "react-router-dom": "^4.3.1",
    "rebass": "^2.3.4",
    "styled-components": "^3.4.10",
    "styled-system": "^3.2.1"
  },
  "devDependencies": {
    "filter-obj": "^2.0.1",
    "map-obj": "^4.1.0",
    "markdown-magic": "2.3.2",
    "nan": "^2.14.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "sane": "^5.0.0",
    "strip-ansi": "^6.0.0"
  }
}
